{
  "name": "warrior-ens-api",
  "version": "2.0.0",
  "description": "Professional ENS API with advanced caching, monitoring, and WebSocket support",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest.e2e.config.js",
    "security:check": "npm audit --production",
    "security:fix": "npm audit fix",
    "docker:build": "docker build -t warrior-ens-api .",
    "docker:run": "docker run -p 3000:3000 -p 8080:8080 warrior-ens-api",
    "clean": "rm -rf dist coverage",
    "prepare": "husky install"
  },
  "keywords": [
    "ens",
    "ethereum",
    "blockchain",
    "api",
    "websocket",
    "ethers",
    "web3",
    "warrior",
    "professional"
  ],
  "author": "Digital Warrior",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.3",
    "ethers": "^6.13.0",
    "ws": "^8.18.0",
    "chalk": "^5.3.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "ioredis": "^5.4.1",
    "node-cache": "^5.1.2",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.0",
    "@types/ws": "^8.5.12",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-security": "^3.0.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "tsx": "^4.19.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.4"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/warrior/ens-api.git"
  },
  "bugs": {
    "url": "https://github.com/warrior/ens-api/issues"
  },
  "homepage": "https://github.com/warrior/ens-api#readme"
}

